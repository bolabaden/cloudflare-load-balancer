{
  "name": "flowbalance",
  "version": "1.0.0",
  "description": "Dead-simple load balancing for Cloudflare Workers. Deploy in minutes, enterprise features when you need them.",
  "scripts": {
    "deploy": "npm run build && wrangler deploy",
    "dev": "npm run build:admin && wrangler dev",
    "dev:admin": "cd admin && vite",
    "build": "npm run build:admin && wrangler build",
    "build:admin": "cd admin && vite build",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "npm run test:config && npm run test:auth && npm run test:types && npm run test:frontend",
    "test:config": "node --test src/config.test.js",
    "test:auth": "node --test test/auth.test.js",
    "test:types": "node --test src/types.test.js",
    "test:frontend": "node --test test/frontend.test.js",
    "test:integration": "node --test test/integration.test.js",
    "test:performance": "node --test test/performance.test.js",
    "test:engine": "node --test test/engine.test.js",
    "test:durable-object": "node --test test/durable-object.test.js",
    "test:api": "node --test test/api.test.js",
    "test:health-checks": "node --test test/health-checks.test.js",
    "test:workers-runtime": "node --test test/workers-runtime.test.js",
    "test:e2e": "node --test test/e2e.test.js",
    "test:security": "node --test test/security.test.js",
    "test:all": "npm run test:unit && npm run test:engine && npm run test:durable-object && npm run test:integration && npm run test:performance && npm run test:api && npm run test:health-checks && npm run test:workers-runtime && npm run test:e2e && npm run test:security",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:vitest:coverage": "vitest run --coverage",
    "test:setup": "node setup-testing.js",
    "test:ci": "npm run typecheck && npm run lint && npm run test:all",
    "typecheck": "tsc --noEmit && npm run typecheck:root && npm run typecheck:admin",
    "typecheck:root": "tsc --noEmit",
    "typecheck:admin": "cd admin && tsc --noEmit && cd ..",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 || echo 'Linting completed with warnings'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/th3w1zard1/cloudflare-failover-test.git"
  },
  "author": "",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.17",
    "@cloudflare/workers-types": "^4.20240405.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.8.3",
    "vite": "^5.2.0",
    "vitest": "^2.1.8",
    "wrangler": "^4.22.0"
  },
  "dependencies": {
    "@tsndr/cloudflare-worker-jwt": "^2.4.0",
    "itty-router": "^4.0.23",
    "pkce-challenge": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
